{
  "name": "vue-admin-template",
  "private": true,
  "version": "0.0.1",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently -k \"yarn dev:vite\" \"yarn dev:view\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron ./src/main/index.js",
    "dev:view": "wait-on tcp:3000 &&cross-env NODE_ENV=development electron ./src/main/index.js",
    "build": "yarn build:vite && yarn pack:main &&electron-builder",
    "build:vite": "vue-tsc --noEmit && vite build",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --config .electron-vue/webpack.main.config.js",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2021 kuari",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "build"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.6",
    "@vue/composition-api": "^1.7.0",
    "core-js": "^3.23.3",
    "element-plus": "^2.2.8",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "babel-loader": "^8.2.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.9.2",
    "vite": "^2.9.9",
    "vue-tsc": "^0.38.3",
    "wait-on": "^6.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
