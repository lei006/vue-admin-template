{
  "name": "report",
  "version": "0.0.1",
  "author": "lei006 <lei006@qq.com>",
  "description": "报告工作站",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "test": "mocha -r babel-register src/test/*.test.js",
    "test:src": "concurrently -k \"yarn dev\" \"yarn test:src\"",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": ""
  },
  "build": {
    "productName": "report",
    "appId": "com.eyoujs.report",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraResources": [
      {
        "from": "./Extensions",
        "to": "../Extensions"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon-app.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@vue/composition-api": "^1.7.0",
    "axios": "^0.21.1",
    "copy-to-clipboard": "^3.3.1",
    "electron-json-storage": "^4.3.0",
    "electron-store": "^7.0.2",
    "element-ui": "^2.15.7",
    "express": "^4.18.1",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.3",
    "nedb": "^1.8.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "qrcodejs2": "0.0.2",
    "random-string": "^0.2.0",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-element-dialog-draggable": "^1.1.0",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.6.2",
    "vuex-electron": "^1.0.3"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "assert": "^2.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^19.0.4",
    "electron-builder": "^20.19.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "listr": "^0.14.3",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^10.0.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.21.0",
    "supertest": "^6.2.3",
    "url-loader": "^1.0.1",
    "vue-devtools": "^5.1.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
